int LED = 6; // create a variable called LED on digital Pin 6

void setup(){
  pinMode(LED, OUTPUT); // declare D pin6 to output 5V
}
  
void loop()
{
  digitalWrite(LED, HIGH); // Send 5V to pin 6 to turn on Light
  delay(200); // wait 200 ms
  digitalWrite(LED, LOW); // send 0V to pin 6 to turn off Light
  delay(200); // wait 200 ms
}
